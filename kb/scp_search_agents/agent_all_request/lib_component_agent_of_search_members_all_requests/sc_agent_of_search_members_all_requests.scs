sc_agent_of_search_members_all_requests
=> nrel_main_idtf:
	[sc-агент поиска всех запросов пользователя] (* <- lang_ru;; *);
	[sc-agent of search Members all requests] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_search_members_all_requests_condition => ..sc_agent_of_search_members_all_requests_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_search_members_all_requests 
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_search_members_all_requests
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_search_members_all_requests
		};;
	-> sc_agent_of_search_members_all_requests_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_search_members_all_requests_condition
= [*
	question_of_search_members_all_requests  _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._param;;
*];;

..sc_agent_of_search_members_all_requests_result
= [*
	question_of_search_members_all_requests  _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._param;;
*];;
