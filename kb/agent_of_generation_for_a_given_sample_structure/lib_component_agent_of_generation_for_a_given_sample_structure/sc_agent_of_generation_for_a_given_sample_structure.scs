sc_agent_of_generation_for_a_given_sample_structure
=> nrel_main_idtf:
	[sc-агент генерации структуры по заданному образцу] 
		(* <- lang_ru;; *);

	[sc-agent of generation for a given sample structure] 
		(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_generation_for_a_given_sample_structure => ..sc_agent_of_generation_for_a_given_sample_structure_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_generation_for_a_given_sample_structure
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_generation_for_a_given_sample_structure
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_generation_for_a_given_sample_structure;
		proc_generation_pair	
		};;
	-> sc_agent_of_generation_for_a_given_sample_structure_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_generation_for_a_given_sample_structure_condition
= [*
	action_of_generation_for_a_given_sample_structure _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;

..sc_agent_of_generation_for_a_given_sample_structure_result
= [*
	action_of_generation_for_a_given_sample_structure _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
