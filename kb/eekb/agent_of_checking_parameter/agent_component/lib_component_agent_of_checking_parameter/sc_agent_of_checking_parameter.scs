sc_agent_of_checking_parameter
=> nrel_main_idtf:
	[агентная scp-программа проверки наличия единицы измерения или шкалы у параметра] (* <- lang_ru;; *);
	[agent scp-program of check unit or scale in the parameter] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_checking_parameter_condition => ..sc_agent_of_checking_parameter_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_checking_parameter
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_checking_parameter
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_checking_parameter;
        proc_of_checking_parameter
		};;
		
	-> sc_agent_of_checking_parameter_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_checking_parameter_condition
= [*
	question_of_checking_parameter _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..sc_agent_of_checking_parameter_result
= [*
	question_of_checking_parameter _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter1;;
*];;
