sc_agent_of_search_using_statements
=> nrel_main_idtf:
	[агентная scp-программа вывода всех утверждений, использующихся для доказательства исходного утверждения] (* <- lang_ru;; *);
	[agent scp-program of display all statements, is used to prove the original statement] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_search_using_statements_condition => ..sc_agent_of_search_using_statements_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_search_using_statements
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_search_using_statements
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_search_using_statements;
        proc_of_search_using_statements
		};;
		
	-> sc_agent_of_search_using_statements_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_search_using_statements_condition
= [*
	question_of_search_using_statements _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..sc_agent_of_search_using_statements_result
= [*
	question_of_search_using_statements _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter1;;
*];;
