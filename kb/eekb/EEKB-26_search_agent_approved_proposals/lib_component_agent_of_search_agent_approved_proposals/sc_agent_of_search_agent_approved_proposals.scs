sc_agent_of_search_agent_approved_proposals
=> nrel_main_idtf:
	[sc-агент поиск всех утвержденных предложений] (* <- lang_ru;; *);
	[sc-agent of search approved proposals] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_search_agent_approved_proposals_condition => ..sc_agent_of_search_agent_approved_proposals_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_search_agent_approved_proposals 
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_search_agent_approved_proposals
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_search_agent_approved_proposals
		};;
	-> sc_agent_of_search_agent_approved_proposals_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_search_agent_approved_proposals_condition
= [*
	action_of_search_agent_approved_proposals  _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._param;;
*];;

..sc_agent_of_search_agent_approved_proposals_result
= [*
	action_of_search_agent_approved_proposals  _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._param;;
*];;
