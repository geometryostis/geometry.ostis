sc_agent_of_creating_draft
=> nrel_main_idtf:	
	[sc-агент создания черновика] (* <- lang_ru;; *);
	[sc-agent of creating draft] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_creating_draft_condition => ..sc_agent_of_creating_draft_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_creating_draft
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_creating_draft
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_creating_draft
		};;
		
	-> sc_agent_of_creating_draft_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_creating_draft_condition
= [*
	action_of_creating_draft _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
*];;

..sc_agent_of_creating_draft_result
= [*
	action_of_creating_draft _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter1;;
	
*];;
