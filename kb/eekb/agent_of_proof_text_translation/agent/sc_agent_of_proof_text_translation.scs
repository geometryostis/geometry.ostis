sc_agent_of_proof_text_translation
=> nrel_main_idtf:
	[sc-агент вывода текста доказательства утверждения на естественном языке.] (* <- lang_ru;; *);
	[sc-agent of the proof text output of the natural language statement.] (* <- lang_ru;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition:
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result:
	(..sc_agent_of_proof_text_translation_initiation_condition => ..sc_agent_of_proof_text_translation_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_proof_text_translation
	};

=> nrel_inclusion:
	.platform_independent_realization_of_sc_agent_of_proof_text_translation
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_proof_text_translation
		};;

	-> sc_agent_of_proof_text_translation_scp
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_proof_text_translation_initiation_condition
= [*
	action_of_proof_text_translation _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_proof_text_translation_result
= [*
	action_of_proof_text_translation _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
