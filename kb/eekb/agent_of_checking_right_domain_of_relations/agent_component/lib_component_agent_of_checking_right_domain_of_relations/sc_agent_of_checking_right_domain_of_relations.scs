sc_agent_of_checking_right_domain_of_relations
=> nrel_main_idtf:
	[агентная scp-программа проверки элементов, участвующих в отношении, на соответствие доменам для указанного отношения] (* <- lang_ru;; *);
	[agent scp-program of checking elements involved in relation to the compliance with the domains for the specified relation] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_checking_right_domain_of_relations_condition => ..sc_agent_of_checking_right_domain_of_relations_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_to_checking_right_domain_of_relations
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_checking_right_domain_of_relations
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_checking_right_domain_of_relations;
		proc_of_addition_binary_arc_construction_in_answer;
		proc_of_formation_binary_common_arc;
		proc_of_formation_binary_edge_arc;
		proc_of_formation_binary_tuple;
		proc_of_formation_of_combinations_for_ternary_relation;
		proc_of_formation_of_ternary_tuple;
		proc_of_formation_quasibinary_tuple;
		proc_of_generating_set_of_domains;
		proc_of_verification_of_binary_arc_domains;
		proc_of_verification_of_domains_of_tuple;
		proc_of_formation_unary_tuple
		};;		

	-> sc_agent_of_checking_right_domain_of_relations_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_checking_right_domain_of_relations_condition
= [*
	action_to_checking_right_domain_of_relations _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_checking_right_domain_of_relations_result
= [*
	action_to_checking_right_domain_of_relations _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
